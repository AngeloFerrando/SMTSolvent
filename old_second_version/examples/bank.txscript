contract Bank {
    resources_bound: 10;
    agents_bound: 2;
    tokens_bound: 1;

    map ( participant -> int ) balances;

    receive() {
        balances[sender] = balances[sender] + value;
    }

    withdraw(amount) {
        require(amount > 0);
        require(amount <= balances[sender]);

        balances[sender] = balances[sender] - amount;
        sender!amount;
    }

}